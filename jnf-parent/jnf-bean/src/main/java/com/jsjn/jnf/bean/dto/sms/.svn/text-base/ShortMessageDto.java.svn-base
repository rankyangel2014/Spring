package com.jsjn.jnf.bean.dto.sms;

import java.io.Serializable;

public class ShortMessageDto implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private String message;
	private String telNo;
	private String msgTmpType;//模板类型

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}

	public String getTelNo() {
		return telNo;
	}

	public void setTelNo(String telNo) {
		this.telNo = telNo;
	}

	public String getMsgTmpType() {
		return msgTmpType;
	}

	public void setMsgTmpType(String msgTmpType) {
		this.msgTmpType = msgTmpType;
	}

}
