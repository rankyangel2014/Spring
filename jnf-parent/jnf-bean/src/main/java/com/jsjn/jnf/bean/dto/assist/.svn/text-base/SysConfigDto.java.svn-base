package com.jsjn.jnf.bean.dto.assist;

import com.jsjn.jnf.bean.dto.base.BaseDTO;

/**
 * @author yincy 系统配置表
 */
public class SysConfigDto extends BaseDTO<SysConfigDto> {

	private static final long serialVersionUID = 1L;

	private String id; //编号
	private String code; //配置项英文编码
	private String describe; //配置项中文描述
	private String value; //配置项值

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDescribe() {
		return describe;
	}

	public void setDescribe(String describe) {
		this.describe = describe;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}
}
