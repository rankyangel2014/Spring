package com.jsjn.jnf.bean.pojo.integration;

import java.math.BigDecimal;

/**
 * 单笔代扣 业务层 --> 集成层 参数
 * 
 * @author yincy
 *
 */
public class SingleWithHoldPojo {

	/**
	 * 收款方(投资人) 在第三方的账号
	 */
	private String investorAccount;

	/**
	 * 收款方(投资人) 在第三方的账号对应的key
	 */
	private String investorKey;

	/**
	 * 付款方(借款人) 身份证号
	 */
	private String custName;

	/**
	 * 付款方(借款人) 身份证号
	 */
	private String custIdNo;

	/**
	 * 付款方(借款人) 银行卡号
	 */
	private String cardNo;

	/**
	 * 付款方(借款人) 银行卡预留手机号
	 */
	private String mobile;

	/**
	 * 付款方(借款人) 开卡银行
	 */
	private String bankName;

	/**
	 * 付款方(借款人) 开卡银行对应的银行编码 备注：不同支付渠道对于的code不同
	 */
	private String bankCode;

	/**
	 * 交易金额
	 */
	private BigDecimal amount;

	/**
	 * 交易订单号
	 */
	private String tradeNo;
	
	/**
	 * 交易时间戳 格式：yyyyMMddHHmmss
	 */
	private String tradeTime;
	
	/**
	 * 交易币种 暂且仅支持人民币
	 */
    private String curCode;
    
    /**
     * 附言 对账用
     */
    private String postScript;
	
	public String getInvestorAccount() {
		return investorAccount;
	}

	public void setInvestorAccount(String investorAccount) {
		this.investorAccount = investorAccount;
	}

	public String getInvestorKey() {
		return investorKey;
	}

	public void setInvestorKey(String investorKey) {
		this.investorKey = investorKey;
	}

	public String getCustName() {
		return custName;
	}

	public void setCustName(String custName) {
		this.custName = custName;
	}

	public String getCustIdNo() {
		return custIdNo;
	}

	public void setCustIdNo(String custIdNo) {
		this.custIdNo = custIdNo;
	}

	public String getCardNo() {
		return cardNo;
	}

	public void setCardNo(String cardNo) {
		this.cardNo = cardNo;
	}

	public String getMobile() {
		return mobile;
	}

	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	public String getBankName() {
		return bankName;
	}

	public void setBankName(String bankName) {
		this.bankName = bankName;
	}

	public String getBankCode() {
		return bankCode;
	}

	public void setBankCode(String bankCode) {
		this.bankCode = bankCode;
	}

	public BigDecimal getAmount() {
		return amount;
	}

	public void setAmount(BigDecimal amount) {
		this.amount = amount;
	}

	public String getTradeNo() {
		return tradeNo;
	}

	public void setTradeNo(String tradeNo) {
		this.tradeNo = tradeNo;
	}

	public String getTradeTime() {
		return tradeTime;
	}

	public void setTradeTime(String tradeTime) {
		this.tradeTime = tradeTime;
	}

	public String getCurCode() {
		return curCode;
	}

	public void setCurCode(String curCode) {
		this.curCode = curCode;
	}

	public String getPostScript() {
		return postScript;
	}

	public void setPostScript(String postScript) {
		this.postScript = postScript;
	}
	
}
