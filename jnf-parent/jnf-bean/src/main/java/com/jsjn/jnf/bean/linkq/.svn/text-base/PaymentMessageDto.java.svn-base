package com.jsjn.jnf.bean.linkq;

import com.jsjn.jnf.common.linkq.LinkQBaseDTO;

/**
 * 转账成功后消息推送
 * 
 * @author xiekx
 * 
 */
public class PaymentMessageDto extends LinkQBaseDTO {

	/**
	 * 
	 */
	private static final long serialVersionUID = -7318287853695708178L;

	private String operate;//操作码
	private String loanNo;//借据号
	private String chargeCode;//推送code
	private String chargeMsg;//推送消息

	public PaymentMessageDto() {
	}

	public String getOperate() {
		return operate;
	}

	public void setOperate(String operate) {
		this.operate = operate;
	}

	public String getLoanNo() {
		return loanNo;
	}

	public void setLoanNo(String loanNo) {
		this.loanNo = loanNo;
	}

	public String getChargeCode() {
		return chargeCode;
	}

	public void setChargeCode(String chargeCode) {
		this.chargeCode = chargeCode;
	}

	public String getChargeMsg() {
		return chargeMsg;
	}

	public void setChargeMsg(String chargeMsg) {
		this.chargeMsg = chargeMsg;
	}
}
